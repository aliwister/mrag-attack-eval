export HF_HOME="/data/hub"
source .venv/bin/activate
export CUDA_VISIBLE_DEVICES=0
nohup python mia.py --runs 3 --dataset_id uclanlp/MRAG-Bench --exp 3 &
export CUDA_VISIBLE_DEVICES=1
nohup python mia.py --runs 3 --dataset_id reach-vb/pokemon-blip-captions --exp 4 &
export CUDA_VISIBLE_DEVICES=2
nohup python mia.py --runs 3 --dataset_id pasindu/google_conceptual_captions_20000 --rag_size 2000 --limit 1000 --exp 3 &
export CUDA_VISIBLE_DEVICES=3
nohup python mia.py --runs 3 --dataset_id eltorio/ROCOv2-radiology --rag_size 2000 --limit 1000 --exp 3 &

export HF_HOME="/data/hub"
source .venv/bin/activate
export CUDA_VISIBLE_DEVICES=1
nohup python icr.py --runs 3 --models Qwen/Qwen2.5-VL-7B-Instruct --exp 3 &
export CUDA_VISIBLE_DEVICES=2
nohup python icr.py --runs 3 --models nvidia/Cosmos-Reason1-7B --exp 3 --datasets uclanlp/MRAG-Bench &
export CUDA_VISIBLE_DEVICES=0
(sleep 1600 && nohup python icr.py --runs 3 --models OpenGVLab/InternVL3_5-8B-HF --exp 3) &

export CUDA_VISIBLE_DEVICES=1
nohup python icr.py --runs 3 --models Qwen/Qwen2.5-VL-7B-Instruct --exp 4 &
export CUDA_VISIBLE_DEVICES=2
nohup python icr.py --runs 3 --models nvidia/Cosmos-Reason1-7B --exp 4 &
export CUDA_VISIBLE_DEVICES=1
nohup python icr.py --runs 3 --models OpenGVLab/InternVL3_5-8B-HF --exp 4 &

export CUDA_VISIBLE_DEVICES=2
nohup python mia.py --runs 3 --dataset_id pasindu/google_conceptual_captions_20000 --rag_size 2000 --limit 1000 --exp 4 &



export CUDA_VISIBLE_DEVICES=1
nohup python icr.py --runs 3 --models Qwen/Qwen2.5-VL-7B-Instruct --exp 1 --is_rerank &
export CUDA_VISIBLE_DEVICES=2
nohup python icr.py --runs 3 --models nvidia/Cosmos-Reason1-7B --exp 1 --is_rerank &
export CUDA_VISIBLE_DEVICES=3
nohup python icr.py --runs 3 --models OpenGVLab/InternVL3_5-8B-HF --exp 1 --is_rerank &


export CUDA_VISIBLE_DEVICES=0
nohup python mia.py --runs 3 --dataset_id pasindu/google_conceptual_captions_20000 --rag_size 2000 --limit 1000 --exp 5 &